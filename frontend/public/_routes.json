{
  "version": 1,
  "include": ["/*"],
  "exclude": [],
  "routes": [
    {
      "src": "^/assets/.+\\.(js|css|png|jpg|jpeg|gif|ico)$",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "^/(.*)",
      "dest": "/index.html"
    }
  ]
}
